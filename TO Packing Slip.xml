<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
	<link name="NotoSans" type="font" subtype="truetype" src="${nsfont.NotoSans_Regular}" src-bold="${nsfont.NotoSans_Bold}" src-italic="${nsfont.NotoSans_Italic}" src-bolditalic="${nsfont.NotoSans_BoldItalic}" bytes="2" />
	<#if .locale == "zh_CN">
		<link name="NotoSansCJKsc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKsc_Regular}" src-bold="${nsfont.NotoSansCJKsc_Bold}" bytes="2" />
	<#elseif .locale == "zh_TW">
		<link name="NotoSansCJKtc" type="font" subtype="opentype" src="${nsfont.NotoSansCJKtc_Regular}" src-bold="${nsfont.NotoSansCJKtc_Bold}" bytes="2" />
	<#elseif .locale == "ja_JP">
		<link name="NotoSansCJKjp" type="font" subtype="opentype" src="${nsfont.NotoSansCJKjp_Regular}" src-bold="${nsfont.NotoSansCJKjp_Bold}" bytes="2" />
	<#elseif .locale == "ko_KR">
		<link name="NotoSansCJKkr" type="font" subtype="opentype" src="${nsfont.NotoSansCJKkr_Regular}" src-bold="${nsfont.NotoSansCJKkr_Bold}" bytes="2" />
	<#elseif .locale == "th_TH">
		<link name="NotoSansThai" type="font" subtype="opentype" src="${nsfont.NotoSansThai_Regular}" src-bold="${nsfont.NotoSansThai_Bold}" bytes="2" />
	</#if>
    <macrolist>
        <macro id="nlheader">
          <table width="100%">
            <tr>
            <td align="left">
               <table class="header" style="width: 50%;">
              <tr>
	<td rowspan="3" style="padding: 0px; margin-top:-10px;"><img src="${'https://5898295.app.netsuite.com/core/media/media.nl?id=3897&amp;c=5898295&amp;h=d84c9437d065122041a5'}" style="width:236.8px;height:52px;"/></td> 
	</tr>
	</table>
              </td>

            <td align="right">
              <table>
                <tr><td style=" font-size: 28pt;margin-right:0px;">Packing Slip</td></tr>
              </table>
              </td>
            </tr>
          </table>
          <table margin-top="10px" width="100%" top="-10px">
            <tr>
            <td align="left">
                <table>
             <tr>
              <td style="padding: 0px;">${companyInformation.companyName}<br />${companyInformation.addressText}<br />${companyInformation.phone}</td>
              </tr>
          </table>
              </td>
              <td align="right">
              <table>
                <tr line-height="75%">
                <td align="left">Transfer Order #</td><td align="right">${record.sonum}</td>
                </tr>
                <tr line-height="75%">
                <td align="left">Date Created</td><td align="right">${record.trandate}</td>
                </tr>

                </table>
              </td>
            </tr>
          </table>
        </macro>
        <macro id="nlfooter">
    <table class="footer" style="width: 100%;margin-top:10px"><tr>
	<td colspan="7">For assistance or questions on this order, please email customercare@exsomed.com</td>
	<td align="right"><pagenumber/> of <totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">* {
		<#if .locale == "zh_CN">
			font-family: NotoSans, NotoSansCJKsc, sans-serif;
		<#elseif .locale == "zh_TW">
			font-family: NotoSans, NotoSansCJKtc, sans-serif;
		<#elseif .locale == "ja_JP">
			font-family: NotoSans, NotoSansCJKjp, sans-serif;
		<#elseif .locale == "ko_KR">
			font-family: NotoSans, NotoSansCJKkr, sans-serif;
		<#elseif .locale == "th_TH">
			font-family: NotoSans, NotoSansThai, sans-serif;
		<#else>
			font-family: Helvetica, sans-serif;
		</#if>
		}
		table {
			font-size: 9pt;
			table-layout: fixed;
		}
		th {
			font-weight: bold;
			font-size: 9pt;
			vertical-align: middle;
            padding: 5px 6px 3px;
            background-color: #e3e3e3;
			color: #333333;
		}
		td {
            padding: 4px 6px;
        }
		td p { align:left }
		b {
			font-weight: bold;
			color: #333333;
		}
		table.header td {
			padding: 0;
			font-size: 10pt;
		}
		table.footer td {
			padding: 0;
			font-size: 8pt;
		}
		table.itemtable th {
			padding-bottom: 10px;
			padding-top: 10px;
		}
		table.body td {
			padding-top: 2px;
		}
		td.addressheader {
			font-size: 8pt;
			padding-top: 6px;
			padding-bottom: 2px;
		}
		td.address {
			padding-top: 0;
		}
		span.title {
			font-size: 22pt;
		}
		span.number {
			font-size: 16pt;
		}
		span.itemname {
			font-weight: bold;
		}
		hr {
			width: 100%;
			color: #d3d3d3;
			background-color: #d3d3d3;
			height: 1px;
		}
        p.itemname1{
        margin-top:80px;
      }
        div.test{
            border-style: solid;
		    border: 1px solid;
		    border-color: gray;
		    width: 4in;
 		    height: 1in;
            margin-top:-5px;
            display:block;
            margin-top:-20px;
      }
</style>
</head>
<body header="nlheader" header-height="15%" footer="nlfooter" footer-height="20pt" padding="0.5in 0.5in 0.5in 0.5in" size="Letter">
  <table width="100%" top="-20px">
     <tr>
        <td align="left"><p style="width: 100%; padding: 0px; margin-top: 10px;align:left;"><b>Ship To</b><br /><span width="50%">${record.shipaddress}</span></p></td>
     </tr>
  </table>

 <table class="body" style="width: 100%; margin-top: 10px;">
   <tr>
    <th font-size="9">Shipping Date</th>
    <th font-size="9">Sales Order #</th>
    <th font-size="9">PO #</th>
	<th font-size="9">${record.shipmethod@label}</th>
	<th font-size="9">Tracking #</th>
	</tr>
	<tr>
    <td>${record.trandate}</td>
    <td>${salesorder.custbody_vr_relatedsalesorder?keep_after("#")}</td>
    <td>${salesorder.custbody_vr_ponumber_to}</td>
	<td>${record.shipmethod}</td>
    <td>${salesorder.linkedtrackingnumbers}</td>
	</tr>
  </table>
<#if record.item?has_content>

<table class="itemtable" style="width: 100%; margin-top: 10px;">
<thead>
	<tr>
	<th colspan="10">Item</th>
    <th colspan="10" align="left">Description</th>
    <th align="center" colspan="4">${salesorder.item[0].quantityordered@label}</th>
    <th colspan="4" align="center">Shipped</th>
	</tr>
</thead>
<!-- start items -->
<#list salesorder.item as tranline><#list record.item as item><#if tranline.line==item.orderline>
  <tr line-height="100%">
    <td colspan="10"><span class="itemname">${tranline.item}</span></td>
    <td colspan="10" padding-left="6px" align="left">${tranline.description}</td>
    <td align="center" colspan="4">${tranline.quantityordered}</td>
    <td colspan="4" align="center">${tranline.quantity}</td>
  </tr>
  <tr  line-height="100%" border-bottom="1px" border-color="#d3d3d3">
    <td colspan="18">
      <#if item.inventorydetail?length != 0>
        <#assign invdetails = item.inventorydetail?split("<br />") />
        <#list invdetails as detail>
          <#if item.isserial = "F">
            <#if detail?index_of(',') gt 0>
              Lot/Serial#: ${detail?split(',')[0]} - Qty: ${detail?split('(')[1]?replace(')','')}
            <#else>
              Lot/Serial#: ${detail?replace('(',' - Qty: ')?replace(')','')}
            </#if>
          <#else>
            Lot/Serial#: ${detail?split(',')[0]}
          </#if>
          <br />
        </#list>
      </#if>
  </td>
  </tr>

  </#if></#list></#list><!-- end items --></table>
</#if>

</body>
</pdf>